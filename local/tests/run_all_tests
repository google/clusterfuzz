#!/bin/bash -ex
SCRIPT_DIR=$(dirname $(readlink -f "$0"))

INTEGRATION=1 $SCRIPT_DIR/run_tests
$SCRIPT_DIR/run_ndb_patcher_tests
$SCRIPT_DIR/run_js_tests
